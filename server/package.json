{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --skipLibCheck && tsc-alias",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "migrate": "node-pg-migrate -m migrations",
    "migrate:create": "npm run migrate create",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "test": "npx jest"
  },
  "keywords": [],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.2",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.7.1",
    "sharp": "^0.34.1",
    "socket.io": "^4.7.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.12.0",
    "jest": "^29.7.0",
    "pg-mem": "^2.8.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "uuid": "^10.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
